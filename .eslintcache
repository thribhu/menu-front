lassname(styles.button200)}\n                onClick={() => history.push(\"/addModifier\")}\n              >\n                Add Modifier\n              </button>\n            </div>\n          </div>\n          <div className={classname(styles.between)}>\n            <div>\n              <button\n                onClick={() => setStep1(false)}\n                className={classname(styles.ctaButton)}\n              >\n                Back\n              </button>\n            </div>\n            <div>\n              <button\n                disabled={!selected.length}\n                className={classname(styles.ctaButton)}\n                onClick={() => (selected.length ? setStep1(false) : null)}\n              >\n                Save\n              </button>\n              <div style={{ fontSize: \"10px\" }}>\n                {!selected.length && (\n                  <p>\n                    <span style={{ color: \"red\" }}>*</span> Select alteast 1\n                    modifier\n                  </p>\n                )}\n              </div>\n            </div>\n          </div>\n        </div>\n      )}\n      {selected.length && !step1 && showSelected ? (\n        <div style={{ flex: 1 }}>\n          <div>\n            <OrderTable\n              columns={columns}\n              data={selected}\n              updateCurrentRows={setNowArray}\n            />\n          </div>\n          <div\n            style={{\n              margin: \"10px auto\",\n              display: \"flex\",\n              justifyContent: \"center\",\n            }}\n          >\n            <button\n              className={styles.ctaButton}\n              onClick={() => {\n                handleSaveItem();\n              }}\n            >\n              {!isEmpty(nowOption) ? \"Update Option\" : \"Add Option\"}\n            </button>\n          </div>\n        </div>\n      ) : null}\n    </div>\n  );\n}\n","/home/manisha/dev/clients/upwork/echola/echola_menu_front/src/modules/groups